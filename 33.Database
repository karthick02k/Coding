CREATE TABLE Students (
    Regno INT PRIMARY KEY,
    name VARCHAR(100),
    address VARCHAR(255),
    age INT,
    mark_1 FLOAT,
    mark_2 FLOAT,
    mark_3 FLOAT,
    mark_4 FLOAT,
    mark_5 FLOAT
);

